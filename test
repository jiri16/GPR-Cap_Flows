foreach x in w_n_bond_hol {

reghdfe `x' gprc gprc_em death_rate scs_sender_mult l1.`x' l2.`x' l3.`x' tr_yield_1y vix gprc_us gr_us_indprod if inrange(ym, ym(1994,4), ym(2024,4)) & ifs!=39, absorb(ifs) vce(cluster ifs)
est store baseline1
reghdfe `x' gprc gprc_em death_rate scs_sender_mult l1.`x' l2.`x' l3.`x' tr_yield_1y vix gprc_us gr_us_indprod gr_indprod inf st_ir gr_er gr_msci  if inrange(ym, ym(1994,4), ym(2024,4))  & ifs!=39, absorb(ifs) vce(cluster ifs)
est store baseline2
reghdfe `x' gprc gprc_em death_rate scs_sender_mult l1.`x' l2.`x' l3.`x' gr_indprod inf st_ir gr_er gr_msci if inrange(ym, ym(1994,4), ym(2024,4))  & ifs!=39, absorb(ifs ym) vce(cluster ifs)
est store baseline3

}

