est store baseline3
xtreg `x' gprc gprc_x_dep_eq dep_eq_gdp l1.`x' l2.`x' l3.`x' gr_indprod inf st_ir gr_er gr_msci i.ym if inrange(ym, ym(1994,4), ym(2021,11)), fe vce(cluster ifs)
est store baseline4
xtreg `x' gprc gprc_x_dep_eq dep_eq_gdp l1.`x' l2.`x' l3.`x' fed vix gr_us_indprod gr_indprod inf st_ir gr_er gr_msci if inrange(ym, ym(1994,4), ym(2021,11)), fe vce(cluster ifs)
