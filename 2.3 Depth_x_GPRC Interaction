foreach x in w_n_bond_hol {

reghdfe `x' gprc gprc_x_dep_bo dep_bo_gdp l1.`x' l2.`x' l3.`x' fed vix gr_reer gr_us_indprod if inrange(ym, ym(1994,4), ym(2021,11)), absorb(ifs) vce(cluster ifs)
est store baseline1
reghdfe `x' gprc gprc_x_dep_bo dep_bo_gdp l1.`x' l2.`x' l3.`x' fed vix gr_us_indprod gr_indprod inf st_ir gr_er gr_msci if inrange(ym, ym(1994,4), ym(2021,11)), absorb(ifs) vce(cluster ifs)
est store baseline2
