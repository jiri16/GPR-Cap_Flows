// collapse to quarterly

gen date = date(month, "MDY")
format date %td
generate qtr = quarter(date)

collapse (last) bt_hol_bond bt_hol_stock gprc tr_yield_1y vix gprc_us gr_us_indprod gr_indprod inf st_ir gr_er gr_msci em (sum) bt_us_net_pur_bond bt_us_net_pur_stock, by(ifs year qtr)





